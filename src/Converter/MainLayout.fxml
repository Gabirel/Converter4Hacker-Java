                            <String fx:value="SHA-1 Encode"/>
                            <String fx:value="SHA-224 Encode"/>
                            <String fx:value="SHA-256 Encode"/>
                            <String fx:value="SHA-384 Encode"/>
                            <String fx:value="SHA-512 Encode"/>
