                    </items>
                </Menu>
            </menus>
        </MenuBar>
        <Label layoutY="29.0" prefHeight="25.0" prefWidth="102.0" text="Original Text:"/>
        <TextArea fx:id="originalTextArea" layoutY="54.0" prefHeight="69.0" prefWidth="600.0"
                  promptText="Input whatever you want to encode" wrapText="true" AnchorPane.leftAnchor="0.0"
                  AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="54.0">
